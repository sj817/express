  "scripts": {
    "build": "tsc && tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "tsx node_modules/mocha/bin/mocha.js --require test/support/env.ts --reporter spec --check-leaks 'test/*.test.ts'",
    "test:router": "tsx node_modules/mocha/bin/mocha.js src/router/test/*.ts",
    "test:body-parser": "tsx node_modules/mocha/bin/mocha.js src/body-parser/test/*.test.ts",
    "test:acceptance": "tsx node_modules/mocha/bin/mocha.js --require test/support/env.ts --reporter spec --check-leaks 'test/acceptance/*.test.ts'",
    "test:all": "tsx node_modules/mocha/bin/mocha.js --require test/support/env.ts --reporter spec --check-leaks test/**/*.test.ts",
    "test-ci": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=lcovonly --reporter=text npm test",
    "test-cov": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=html --reporter=text npm test",
    "test-tap": "tsx node_modules/mocha/bin/mocha.js --require test/support/env.ts --reporter tap --check-leaks 'test/*.test.ts'"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  }
